<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="structBean" namespace="/" extends="struts-default">
        <action name="login" class="org.example.web.LoginAction" method="login">
            <result name="success">suc.jsp</result>
            <result name="fail">login.jsp</result>
            <result name="input">login.jsp</result>
        </action>

        <action name="register" class="org.example.web.LoginAction" method="register">
            <result name="success">regSuccess.jsp</result>
            <result name="fail">error.jsp</result>
        </action>
    </package>


</struts>