        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.moredatasorce.dao.master.Test1">


    <select id="getclass1" resultType="com.example.moredatasorce.entity.Class">
       select * from class
   </select>

